<?xml version='1.0' encoding='utf-8'?>
<MixxxControllerPreset schemaVersion="1" mixxxVersion="1.11+">
    <info>
        <name>Open Turntable</name>
        <author>Thomas Niedzwiadek and Michael Pavkovic</author>
        <description>Open Turntable Mixxx bindings</description>
    </info>
    <controller id="Mbed">
        <scriptfiles>
            <file filename="open-turntable-scripts.js" functionprefix="OpenTurntable"/>
        </scriptfiles>
        <controls>
            <control>
                <group>[Channel1]</group>
                <key>play</key>
                <description>Play</description>
                <status>0x90</status>
                <midino>0x0B</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>rate</key>
                <description>Rate</description>
                <status>0xB0</status>
                <midino>0x19</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>crossfader</key>
                <description>Crossfader</description>
                <status>0xB0</status>
                <midino>0x20</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>cue_default</key>
                <description>Cue</description>
                <status>0x90</status>
                <midino>0x0C</midino>
                <options>
                    <normal/>
                </options>
            </control>
            <control>
                <group>[Channel1]</group>
                <key>OpenTurntable.wheelTurn</key>
                <description>Wheel Turn Positive</description>
                <status>0xB0</status>
                <midino>0x21</midino>
                <options>
                    <script-binding/>
                </options>
            </control>
        </controls>
        <outputs/>
    </controller>
</MixxxControllerPreset>
